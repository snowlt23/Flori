
import "core/prelude"
import "tests/testutils"

expect("")

fn infix_test() {
  assert(1 == 1)
  assert(1 == 1 && 1 != 2)
  assert(1 + 1 == 2 && 4*5 == 20)
  assert(1 - 2 == (-1) && int(20/5) == 4)
}

fn while_test() {
  a := 0
  while (a < 10) {
    a += 1
  }
  assert(a == 10)
}

fn cstring_test() {
  assert("yukari!" == "yukari!")
  assert("yukari!" != "ia!")
}

fn main_test() {
  infix_test()
  while_test()
  
}
main_test()
