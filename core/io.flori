
import "core/prelude"

fn print(fmt CString) $[importc "printf", header "stdio.h"]
fn printf[T](fmt CString, x T) $[importc "printf", header "stdio.h"]

fn print(x Char) {
  printf("%c", x)
}
fn print(x Int) {
  printf("%d", x)
}
fn print(x UInt8) {
  printf("%d", x)
}
fn print(x Float) {
  printf("%f", x)
}
fn print(x Bool) {
  if (x) {
    print("true")
  } else {
    print("false")
  }
}
fn print(x Clock) {
  printf("%lld", x)
}

fn println[T](x T) {
  print(x)
  print("\n")
}
