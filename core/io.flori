
(require prelude :refer :all)

@(^ (:a All))
@(: CString :a)
(c-import printf :name "printf" :header "stdio.h")
@(: CString)
(c-import print :name "printf" :header "stdio.h")

@(: UInt8)
(defn print [x]
  (printf "%c" x))
@(: Int32)
(defn print [x]
  (printf "%d" x))
@(: UInt32)
(defn print [x]
  (printf "%u" x))
@(: Bool)
(defn print [x]
  (if x
    (print "true")
    (print "false")))
