
import "core/prelude"

fn env_setup() $[importc, header nodeclc, pattern "
#ifdef _WIN32
#define is_windows true
#elif _WIN64
#define is_windows true
#else
#define is_windows false
#endif
"]
fn is_windows_define() Bool $[importc, header nodeclc, pattern "is_windows"]
fn is_windows() Bool {
  env_setup()
  is_windows_define()
}
