
import "core/prelude"

fn is_windows() Bool $[importc, header nodeclc, declc "
#ifdef _WIN32
#define is_windows true
#elif _WIN64
#define is_windows true
#else
#define is_windows false
#endif
", pattern "is_windows"]

fn is_compiletime() Bool $[importc, header nodeclc, declc "
#ifdef FLORI_COMPILETIME
#define is_compiletime true
#else
#define is_compiletime false
#endif
", pattern "is_compiletime"]
fn is_runtime() Bool {
  not(is_compiletime())
}
