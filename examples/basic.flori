`+` => $infix $typed(int, int) $internal("int_add")
`+.` => $infix $typed(float, float) $internal("float_add")

fadd_sugg => $trigger(typetrig(`+`, float, float))
  hint("can't use `+` operator as float add, should be use `+.` operator.")

fib(n) =>
  if n<2:
    n
  else:
    fib(n-1) + fib(n-2)

