    vector_`a` => $struct(data: array(1024, `a`), len: int)
    vector_`a`_len(v) => v.len
    vector_`a`_push(v, e) => 
  quote:
    vector_`a`
push(v, e) => $codegen
  quote:
    vector_`typeof(v)`_push(`v`, `e`)
len(v) => $codegen
  quote:
    vector_`typeof(v)`_len(`v`)

vector(a) => $template
  vector(a) => $struct(data: array[1024, `a`], len: int)
  len(v) => v.len
  push(v, e) =>

`..` => $infix $typed(int, int)

iota(s, e) =>
  v := vector(int)
  for i in s..e:
    push(v, i)
  v

