struct myint {
  a ^int
  b ^int
}

inline fn *=(p ^ptr ptr myint, v ^ptr myint) {
  pop_rcx() pop_rax()
  X(0x48) X(0x89) X(0x08)
}

fn init_myint(result ^ptr myint) {
  result.a = 4
  result.b = 6
}

fn main() {
  var mi ^myint
  mip := &mi
  mip.a = 1
  mip.b = 2
  x := mi.a + mi.b
  init_myint(&mi)
  mi.a + mi.b + x
}
