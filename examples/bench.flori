
import core/prelude
import core/io
import core/macros

macro bench(fexpr FExpr) FExpr {
  body := access(fexpr, 1)
  quote {
    starttime := get_clock()
    `body
    print("Elapsed: ")
    println((get_clock() - starttime) / 1000)
  }
}

fn fib(n Int) Int {
  if (n < 2) {
    n
  } else {
    fib(n-1) + fib(n-2)
  }
}

bench {
  println(fib(30))
}
