}

macro def_add5() FExpr {
  quote {
    fn add5(x Int) Int {
      x + 5
    }
  }
}

macro mac(x Int) FExpr {
  quote {
    "Int!"
  }
}
macro mac(x CString) FExpr {
  quote {
    "String!"
  }
}


hello := "Hello Yukari!"
