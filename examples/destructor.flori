
import "core/prelude"
import "core/io"
import "core/syntax"

type MyInt {
  x Int
  y Int
}

destructor(mi MyInt) {
  print("MyInt(")
  print(mi.x)
  print(", ")
  print(mi.y)
  println(") destroyed!")
}

fn main() {
  mi := init(MyInt){1, 2}
  mi2 := init(MyInt){3, 4}
  mi = mi2
  println(mi.x)
  println(mi.y)
}

main()
