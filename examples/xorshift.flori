
(require prelude :refer :all)
(require io :refer :all)

(var x (uint32 123456789))
(var y (uint32 362436069))
(var z (uint32 521288629))
(var w (uint32 88675123))

@(: -> UInt32)
(defn xor128 []
  (var t (pow x (<< x (uint32 11))))
  (set! x y)
  (set! y z)
  (set! z w)
  (set! w
    (pow
      (pow w (>> w (uint32 19)))
      (pow t (>> t (uint32 8)))))
  w)

(print (xor128))
(print "\n")
(print (xor128))
