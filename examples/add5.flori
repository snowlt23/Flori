@(: Int32 -> Int32)
(defn add5 [x]
  (var res (+ x 5))
  (abs res))

@(: Int32 Bool Bool -> Int32)
(defn abstrue [x b1 b2]
  (if b1
    (if b2
      (abs x)
      x)
    x))

