
import "core"
import "core/pointer"

default_region_settings()

fn pint(x Int32) Ptr[Int32] {
  i := alloc[Int32]()
  unref(i) = x
  i
}

fn main() {
  for i in range(1, 1000000) {
    in_region {
      v := vec[Ptr[Int32]]()
      push(v, pint(1))
      push(v, pint(2))
      push(v, pint(3))
      push(v, pint(4))
      push(v, pint(5))
    }
  }
}

main()
